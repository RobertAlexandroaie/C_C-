<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ICustomer" />
                <binding name="BasicHttpBinding_IReportService" />
                <binding name="BasicHttpBinding_IOrder" />
            </basicHttpBinding>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_ISubscribe" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/ReportServiceCO/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IReportService"
                contract="ProxyReport.IReportService" name="BasicHttpBinding_IReportService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/Subscribe/SubscribeMessage/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_ISubscribe"
                contract="ProxySubscribe.ISubscribe" name="WSDualHttpBinding_ISubscribe">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/CustomerOrder/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ICustomer"
                contract="Proxy.ICustomer" name="BasicHttpBinding_ICustomer" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/CustomerOrder/Service1/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IOrder"
                contract="Proxy.IOrder" name="BasicHttpBinding_IOrder" />          
        </client>
    </system.serviceModel>
</configuration>