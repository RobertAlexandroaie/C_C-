<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <services>
      <service name="CustomerOrder.CustomerOrder">
        <endpoint address="net.tcp://localhost:8182/customer" binding="netTcpBinding"
            contract="CustomerOrder.ICustomer" listenUriMode="Explicit">
        </endpoint>
        <endpoint address="net.tcp://localhost:8182/customer" binding="netTcpBinding"
            bindingConfiguration="" contract="CustomerOrder.IOrder" />
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange" />
      </service>
      <service name="DatabaseClient">
        <clear />
        <endpoint address="net.tcp://localhost:8181/database" binding="netTcpBinding"
            bindingConfiguration="" contract="Database" />
        <endpoint address="mex" binding="mexTcpBinding" contract="IMetadataExchange" />
      </service>
    </services>
  </system.serviceModel>
</configuration>